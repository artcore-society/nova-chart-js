{
    "name": "artcore-society/nova-chart-js",
    "description": "nova-chartjs fork to support Nova 5",
    "keywords": [
        "laravel",
        "nova",
        "stacked chart",
        "chart js"
    ],
    "license": "MIT",
    "homepage": "https://github.com/coroo",
    "authors": [
        {
            "name": "Kuncoro Wicaksono",
            "email": "coroo.wicaksono@gmail.com",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "php": ">=8",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Coroowicaksono\\ChartJsIntegration\\": "src/",
            "Coroowicaksono\\ChartJsIntegration\\Api\\": "src/api"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coroowicaksono\\ChartJsIntegration\\CardServiceProvider"
            ]
        },
        "forked-from": "coroo/nova-chartjs"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
